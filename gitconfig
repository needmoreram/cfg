[user]
    name = Ram Subramanian
    email = ram@meraki.com
[color]
    ui = true
    status = auto
[diff]
    tool = vimdiff
    algorithm = patience
[core]
    editor = /usr/bin/vim
[alias]
    st = status --short
    dc = diff --cached
    ll = log --pretty=format:'%C(yellow)%h%Creset - %Cblue%an%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative --all     # Optionally, give --since
    llg = log --pretty=format:'%C(yellow)%h%Creset - %Cblue%an%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative --all --graph
    co = checkout
[push]
    default = simple
[branch]
    autosetuprebase = always
